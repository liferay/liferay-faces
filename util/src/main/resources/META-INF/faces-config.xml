<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:bridge="http://www.liferay.com/xml/ns/liferay-faces-bridge-2.0-extension" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<name>LiferayFacesUtil</name>
	<!-- Ordering must be "after" ICEfacesCore because the com.liferay.faces.util.context.PartialViewContexCleanupImpl -->
	<!-- instance needs to be able to wrap the ICEfaces org.icefaces.impl.context.DOMPartialViewContext instance. -->
	<ordering>
		<after>
			<name>ICEfacesCore</name>
		</after>
	</ordering>
</faces-config>
