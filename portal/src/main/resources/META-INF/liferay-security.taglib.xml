<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
   http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://liferay.com/faces/security</namespace>
	<tag>
		<description>The &lt;code&gt;liferay-security:permissionsURL&lt;/code&gt; tag renders an HTML anchor tag (hyperlink) that the user can click on in order to see the Liferay Permissions screen for the associated resource.</description>
		<tag-name>permissionsURL</tag-name>
		<component>
			<component-type>com.liferay.faces.portal.PermissionsURL</component-type>
			<renderer-type>com.liferay.faces.portal.PermissionsURLRenderer</renderer-type>
		</component>
	</tag>
</facelet-taglib>
