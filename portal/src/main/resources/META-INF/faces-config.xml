<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
	<application>
		<el-resolver>com.liferay.faces.portal.el.ExtELResolver</el-resolver>
		<resource-handler>com.liferay.faces.portal.resource.LiferayFacesResourceHandler</resource-handler>
		<resource-handler>com.liferay.faces.portal.resource.LiferayPortalResourceHandler</resource-handler>
	</application>
	<factory>
		<partial-view-context-factory>com.liferay.faces.util.context.PartialViewContextFactoryCleanupImpl</partial-view-context-factory>
	</factory>
	<component>
		<component-type>com.liferay.faces.portal.PermissionsURL</component-type>
		<component-class>com.liferay.faces.portal.component.PermissionsURL</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.portal.InputEditorInternal</component-type>
		<component-class>com.liferay.faces.portal.component.InputEditorInternal</component-class>
	</component>
	<lifecycle>
		<phase-listener>com.liferay.faces.portal.lifecycle.LiferayLocalePhaseListener</phase-listener>
	</lifecycle>
	<render-kit>
		<renderer>
			<component-family>com.liferay.faces.portal.PermissionsURL</component-family>
			<renderer-type>com.liferay.faces.portal.renderkit.PermissionsURLRenderer</renderer-type>
			<renderer-class>com.liferay.faces.portal.renderkit.PermissionsURLRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>com.liferay.faces.portal.InputEditorInternal</component-family>
			<renderer-type>com.liferay.faces.portal.renderkit.InputEditorInternalRenderer</renderer-type>
			<renderer-class>com.liferay.faces.portal.renderkit.InputEditorInternalRenderer</renderer-class>
		</renderer>
	</render-kit>
	<validator>
		<validator-id>com.liferay.faces.portal.CaptchaValidator</validator-id>
		<validator-class>com.liferay.faces.portal.validator.CaptchaValidator</validator-class>
	</validator>
	<validator>
		<validator-id>com.liferay.faces.portal.RichTextLengthValidator</validator-id>
		<validator-class>com.liferay.faces.portal.validator.RichTextLengthValidator</validator-class>
	</validator>
</faces-config>
