	version="2.0">
	<description>The Liferay Faces Bridge facelet component tags with the &lt;code&gt;bridge:&lt;/code&gt; tag name prefix.</description>
	<namespace>http://liferay.com/faces/bridge</namespace>
	<tag>
		<description>Renders the HTML tag: &lt;xmp&gt;&lt;input type=&quot;file&quot; /&gt;&lt;/xmp&gt; The tag provides file upload capability provides file upload capability. Usage of this tag requires the Apache &lt;code&gt;commons-fileupload&lt;/code&gt; and &lt;code&gt;commons-io&lt;/code&gt; dependencies.</description>
		<tag-name>inputFile</tag-name>
		<component>
			<component-type>javax.faces.HtmlInputFile</component-type>
			<renderer-type>com.liferay.faces.bridge.renderkit.bridge.InputFileRenderer</renderer-type>
			<handler-class>com.liferay.faces.bridge.component.HtmlInputFileHandler</handler-class>
		</component>
		<!-- Attributes related to type="file" -->
		<attribute>
			<name>accept</name>
			<type>java.lang.String</type>
		</attribute>
		<!-- Standard HTML input attributes -->
		<attribute>
			<name>accesskey</name>
			<type>java.lang.String</type>
		</attribute>
		<!-- Standard Faces attributes -->
		<attribute>
			<description>An EL expression that represents a JavaBean property for the corresponding HtmlInputFile component in the runtime component tree.</description>
			<name>binding</name>
			<required>true</required>
			<type>com.liferay.faces.bridge.component.HtmlInputFile</type>
		</attribute>
		<!-- Standard HTML input attributes cont. -->
		<attribute>
			<name>dir</name>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>disabled</name>
			<type>java.lang.String</type>
		</attribute>
		<!-- Attributes related to type="file" cont. -->
		<attribute>
			<name>fileUploadListener</name>
			<required>false</required>
			<type>javax.el.MethodExpression</type>
		</attribute>
		<!-- Standard Faces attributes cont. -->
		<attribute>
			<description>The identifier of the component.</description>
			<name>id</name>
			<required>false</required>
			<type>java.lang.String</type>
		</attribute>
		<!-- Standard HTML input attributes cont. -->
		<attribute>
			<name>lang</name>
			<type>java.lang.String</type>
		</attribute>
		<!-- HTML5 attributes -->
		<attribute>
			<name>multiple</name>
			<type>java.lang.String</type>
		</attribute>
