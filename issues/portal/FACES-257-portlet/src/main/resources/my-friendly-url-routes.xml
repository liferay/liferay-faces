<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
	<route>
		<pattern>/{alpha}/my-friendly-action/{beta:\d+}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
		<implicit-parameter name="gamma">0</implicit-parameter>
	</route>
	<route>
		<pattern>/{alpha}/my-friendly-action/{beta:\d+}/{gamma}</pattern>
		<ignored-parameter name="_facesViewIdRender" />
	</route>
</routes>
