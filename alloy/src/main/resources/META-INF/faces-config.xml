<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	<application>
		<el-resolver>com.liferay.faces.alloy.el.AUIELResolver</el-resolver>
	</application>
	<component>
		<component-type>com.liferay.faces.alloy.AUIButtonRow</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIButtonRow</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIColumn</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIColumn</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIDialog</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIDialog</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIField</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIField</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIFieldSet</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIFieldSet</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIForm</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIForm</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUILayout</component-type>
		<component-class>com.liferay.faces.alloy.component.AUILayout</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIList</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIList</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIListItem</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIListItem</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUITextBoxList</component-type>
		<component-class>com.liferay.faces.alloy.component.AUITextBoxList</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUITextBoxListItem</component-type>
		<component-class>com.liferay.faces.alloy.component.AUITextBoxListItem</component-class>
	</component>
	<converter>
		<converter-id>convertLongList</converter-id>
		<converter-class>com.liferay.faces.alloy.converter.LongListConverter</converter-class>
	</converter>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.ButtonRowRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.ButtonRowRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.ColumnRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.ColumnRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.DialogRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.DialogRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.FieldRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.FieldRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.FieldSetRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.FieldSetRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.LayoutRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.LayoutRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.ListRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.ListRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.ListItemRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.ListItemRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.TextBoxListRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.TextBoxListRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.TextBoxListItemRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.TextBoxListItemRenderer</renderer-class>
		</renderer>
	</render-kit>

</faces-config>
