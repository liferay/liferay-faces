<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	xmlns:util="http://www.liferay.com/xml/ns/liferay-faces-util-extension">

	<name>LiferayFacesAlloy</name>
	<ordering>
		<after>
			<name>LiferayFacesUtil</name>
		</after>
	</ordering>
	<application>
		<el-resolver>com.liferay.faces.alloy.el.AUIELResolver</el-resolver>
		<system-event-listener>
			<system-event-listener-class>com.liferay.faces.alloy.component.inputfile.internal.InputFileRenderer</system-event-listener-class>
			<system-event-class>com.liferay.faces.util.event.PostConstructApplicationConfigEvent</system-event-class>
		</system-event-listener>
	</application>
	<component>
		<component-type>com.liferay.faces.alloy.AUIButtonRow</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIButtonRow</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIColumn</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIColumn</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUICol</component-type>
		<component-class>com.liferay.faces.alloy.component.AUICol</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIRow</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIRow</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIField</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIField</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIFieldSet</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIFieldSet</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIForm</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIForm</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUILayout</component-type>
		<component-class>com.liferay.faces.alloy.component.AUILayout</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIList</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIList</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIListItem</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIListItem</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUITextBoxList</component-type>
		<component-class>com.liferay.faces.alloy.component.AUITextBoxList</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUITextBoxListItem</component-type>
		<component-class>com.liferay.faces.alloy.component.AUITextBoxListItem</component-class>
	</component>
	<component>
		<component-type>com.liferay.faces.alloy.AUIScript</component-type>
		<component-class>com.liferay.faces.alloy.component.AUIScript</component-class>
	</component>
	<converter>
		<converter-id>convertLongList</converter-id>
		<converter-class>com.liferay.faces.alloy.converter.LongListConverter</converter-class>
	</converter>
	<factory>
		<factory-extension>
			<util:component-resource-factory>com.liferay.faces.alloy.application.internal.ComponentResourceFactoryAlloyImpl</util:component-resource-factory>
			<util:message-context-factory>com.liferay.faces.alloy.context.internal.MessageContextFactoryAlloyImpl</util:message-context-factory>
		</factory-extension>
	</factory>
	<render-kit>
		<renderer>
			<description>Overrides the BodyRenderer that comes with the JSF implementation</description>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>javax.faces.Body</renderer-type>
			<renderer-class>com.liferay.faces.alloy.component.body.internal.BodyRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ButtonRowRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ButtonRowRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ColumnRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ColumnRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ColRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ColRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.RowRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.RowRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.FieldRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.FieldRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.FieldSetRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.FieldSetRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.LayoutRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.LayoutRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ListRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ListRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ListItemRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ListItemRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.TextBoxListRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.TextBoxListRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.TextBoxListItemRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.TextBoxListItemRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>com.liferay.faces.alloy.renderkit.internal.ScriptRenderer</renderer-type>
			<renderer-class>com.liferay.faces.alloy.renderkit.internal.ScriptRenderer</renderer-class>
		</renderer>
	</render-kit>

</faces-config>
