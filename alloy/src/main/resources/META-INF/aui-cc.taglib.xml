<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
   http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<description>The Liferay Faces Alloy facelet composite component tags with the &lt;code&gt;aui-cc:&lt;/code&gt; tag name prefix.</description>
	<namespace>http://liferay.com/faces/aui-cc</namespace>
	<composite-library-name>aui-cc</composite-library-name>
	<function>
		<description>Returns a string of CSS classes with the second argument appended to each CSS class in the first argument.</description>
		<function-name>appendToCssClasses</function-name>
		<function-class>com.liferay.faces.alloy.util.AlloyUtil</function-class>
		<function-signature>java.lang.String appendToCssClasses(java.lang.String, java.lang.String)</function-signature>
	</function>
</facelet-taglib>
